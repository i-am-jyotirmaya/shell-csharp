{
    "version": "0.2.0",
    "configurations": [
      {
        "name": ".NET Core Launch (console with arguments)",
        "type": "coreclr",
        "request": "launch",
        "program": "${workspaceFolder}/bin/Debug/net8.0/codecrafters-shell.dll",
        "args": [
            "echo \"Hello World\""
        ],
        "cwd": "${workspaceFolder}",
        "stopAtEntry": false,
        "console": "integratedTerminal",
        "preLaunchTask": "build"
      }
    ]
  }
  